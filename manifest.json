{
    "manifest_version": 3,
    "name": "File Classifier",
    "version": "1.0",
    "description": "Classifies downloaded files as audio, video, or documents and stores them in respective folders.",
    "permissions": [
      "downloads"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/icon16.png"
      }
    },
    "icons": {
      "16": "images/icon16.png"
    }
  }
  